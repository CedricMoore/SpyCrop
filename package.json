{
  "name": "spy-crop",
  "version": "0.0.0",
  "main": "main.js",
  "watch": {
    "test": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js,html,scss,ts",
      "ignore": "src/vendor/external.min.js",
      "quiet": false,
      "legacyWatch": false,
      "delay": 0,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "autobuild": "ng build -c development --watch --build-optimizer=false",
    "el": "electron .",
    "elbuild": "electron-rebuild",
    "watch": "npm-watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@types/electron": "^1.6.10",
    "material-design-icons-iconfont": "^6.1.0",
    "ngx-electron": "^2.2.0",
    "npm-watch": "^0.10.0",
    "path": "^0.12.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "watch": "^1.0.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.0",
    "@angular-devkit/build-angular": "^12.0.5",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^15.12.5",
    "electron": "^13.0.0",
    "electron-rebuild": "^2.3.5",
    "electron-reloader": "^1.2.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
